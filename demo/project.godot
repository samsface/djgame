config_version=5

[application]

config/name="Demo"
run/main_scene="res://game/game.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

NodeDb="*res://addons/libpd/script/objects/node_db.gd"
SelectionBus="*res://addons/libpd/script/selection_bus.gd"
PureData="*res://addons/libpd/pure_data.gd"
GameTime="*res://models/phone/clock.gd"
Bus="*res://bus.gd"

[display]

window/size/viewport_width=1680
window/size/viewport_height=1050
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_sfxr/plugin.cfg", "res://addons/spread/plugin.cfg", "res://addons/verlet_physics/plugin.cfg")

[filesystem]

import/blender/enabled=false

[gui]

theme/default_font_multichannel_signed_distance_field=true

